body {
  margin: 0px;
}
a {
  color: #000000;
}
form {
  display: inline;
}
#head {
  height: 15%;
  width: 100%;
  float: left;
  background-image: url(../medias/images/head_back.png);
  background-size: auto 100%;
  text-align: center;
  overflow: auto;
}
#content {
  height: 77%;
  width: 100%;
  float: left;
  background-color: #E8E8E8;
}
#menu {
  height: 100%;
  width: 15%;
  float: left;
  background-image: url(../medias/images/menu_back.png);
  background-size: 100% 100%;
}
#member {
  width: 100%;
}
#corps {
  width: 84%;
  height: 100%;
  margin-left: 1%;
  float: left;
  overflow: auto;
}
#foot {
  height: 8%;
  width: 100%;
  float: left;
  background-image: url(../medias/images/foot_back.png);
  background-size: auto 100%;
  text-align: center;
}
#disconnect {
  float: right;
}
